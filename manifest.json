{
    "manifest_version": 2,
    "name": "hudl-dev-sensei",
    "version": "0.1.0",
    "description": "Adds a Nomination button to Hudl pull requests.",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["https://github.com/hudl/*/pulls/*"],
        "js": ["jquery.min.js", "contentscript.js"]
    }],
    "author": "Jordan Degner", 
    "permissions": [
        "tabs",
        "https://github.com/hudl/*/pull/*"
    ]
}